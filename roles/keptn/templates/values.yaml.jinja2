certManager:
  enabled: {{ keptn_certmanager_enabled | lower }}
lifecycleOperator:
  enabled: {{ keptn_lifecycleoperator_enabled | lower }}
metricsOperator:
  enabled: {{ keptn_metricsoperator_enabled | lower }}

global:
  imageRegistry: {{ keptn_global_imageregistry | quote }}

  imagePullSecrets: {{ keptn_global_imagepullsecrets }}
  imagePullPolicy: ""
  commonLabels: {{ keptn_global_commonlabels | to_json }}
  commonAnnotations: {{ keptn_global_annotations | to_json }}
  caInjectionAnnotations: {}
